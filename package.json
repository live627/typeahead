{
  "name": "typeahead",
  "version": "0.2.0",
  "description": "typeahead component",
  "main": "src/typeahead.js",
  "scripts": {
    "build": "mkdirp build && (uglifyjs --compress booleans  --mangle --reserved $ --source-map build/typeahead.min.js.map -- src/typeahead.js > build/typeahead.min.js & lessc src/typeahead.less > build/typeahead.css)",
    "watch": "watch \"npm run build\" src",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "https://live627@github.com/live627/typeahead.git"
  },
  "keywords": [
    "autosuggest",
    "typeahead",
    "jquery",
    "jquery-plugin"
  ],
  "author": "live627 <live627@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/live627/typeahead/issues"
  },
  "homepage": "https://github.com/live627/typeahead",
  "devDependencies": {
    "less": "^2.5.0",
    "uglify-js": "^2.4.20",
    "watch": "^0.16.0"
  },
  "dependencies": {
    "mkdirp": "^0.5.0"
  }
}
